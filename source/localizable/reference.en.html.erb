<script type="text/javascript">
	var nav_height = $('.navigation #logo').height();
	var mobile;

	$(window).width() > 768 ? mobile = false : mobile = true;

	function circle(width, height, target, name) {
		this.elm = $('<a id="bubble_' + name + '" ' + (mobile ? '' : 'class="circle"') + '>' +
			'<div class="inner"></div>' +
			'</a>');
		this.width = width;
		this.height = height;
		this.top = null;
		this.left = null;
		this.angle = Math.random() * 360 * Math.PI / 180;
		this.speed = 0.5;
		this.xvel = Math.cos(this.angle) * this.speed;
		this.yvel = Math.sin(this.angle) * this.speed;
		this.target = $(target);
		this.name = name;
		this.init = function() {
			if (!mobile) {
				this.elm.css({
					'width': this.width,
					'height': this.height
				});
			}

			// Click bubble = scroll animation
			$(this.elm[0]).click(function(){

				var id = $(this).find('img').attr('class');
				var obj = $('div#'+id).find('img');
				var href = $('div#'+id).children('a').attr('href');
				var sTop = obj.offset().top;

				if(href !== undefined){
					window.location.href = href;
				}else{
					obj.addClass((obj.hasClass('animated')) ? false : 'animated').removeClass('flash');

					$('html, body').animate({
			        scrollTop: sTop - 200
			    }, 1200, 'easeInOutExpo', function(){
			    	obj.addClass('flash');
			    });
				}

			});
			// End scroll animation

			this.elm.appendTo(this.target);
			this.top = Math.random() * (this.target.height() - this.elm.height() - nav_height - 80) + nav_height;
			this.left = Math.random() * (this.target.width() - this.elm.width());
			this.elm.find('.inner').append('<img class="' + this.name + '" src="images/Refs/' + this.name + '.png" alt="ref" />');
		}
		this.update = function(){
			this.top += this.yvel;
			this.left += this.xvel;

			this.elm.css({
				'top': Math.round(this.top),
				'left': Math.round(this.left)
			});

			if (this.top < nav_height || this.top > this.target.height() - this.elm.height() - 80) {
				this.yvel = -this.yvel;
			}
			if (this.left < 0 || this.left > this.target.width() - this.elm.width()) {
				this.xvel = -this.xvel;
			}
		}
	}

	var circles = [];

	circles.push(new circle('14vw', '14vw', '.desktop', 'greenswan'));
	circles.push(new circle('14vw', '14vw', '.desktop', 'partspoint'));
	circles.push(new circle('14vw', '14vw', '.desktop', 'cemio'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'modry-andel'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'procare'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'eurosignal'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'cia-news'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'avu'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'vegatour'));
	circles.push(new circle('12vw', '12vw', '.desktop', 'autoprima'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'toptrika'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'domenacz'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'magistra'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'hasbro'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'ignum'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'bahal'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'amca'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'hasek'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'extenso'));
	circles.push(new circle('9vw', '9vw', '.desktop', 'paartyy'));

	for (var i = 0; i < circles.length; i++) {
		circles[i].init();
	}

	function render() {
		for (var i = 0; i < circles.length; i++) {
			circles[i].update();
		}
	}

	if (!mobile) {
		window.requestAnimFrame = (function() {
			return window.requestAnimationFrame ||
				window.webkitRequestAnimationFrame ||
				window.mozRequestAnimationFrame ||
				function(callback) {
					window.setTimeout(callback, 1000 / 60);
				};
		})();

		// (function anim() {
		// 	requestAnimFrame(anim);
		// 	render();
		// })();
	}

	// Arrow under bubbels
	$(window).scroll(function(){
		if($(window).height() - 200 <= $(window).scrollTop())
			$('#arrow_down').fadeOut(600);
		else if($(window).scrollTop() <= 10)
			$('#arrow_down').fadeIn(600);
	});

	$('#arrow_down').click(function(){
		$('html, body').animate({
	        scrollTop: $(window).height() - 200
	    }, 1200, 'easeInOutExpo');
	});
</script>
